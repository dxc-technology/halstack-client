{
  "name": "@dxc-technology/halstack-client",
  "version": "1.3.2",
  "description": "DXC Technology Halstack Client",
  "repository": {
    "type": "git",
    "url": "https://github.com/dxc-technology/dxc-halstack-client"
  },
  "main": "./main.js",
  "scripts": {
    "bundle": "rollup -c",
    "copy": "node ./scripts/build/copy-distribution-files.js",
    "start": "rollup -c -w",
